<%if(successMsg.length){%>
     <div class="alert alert-success alert-dismissible fade show shadow-lg rounded p-3 col-md-6 offset-md-3" role="alert">
       <%=successMsg%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
    <%}%>
<% if (errorMsg.length > 0) { %>
    <div class="alert alert-danger alert-dismissible fade show shadow-lg rounded p-3 col-md-6 offset-md-3" role="alert">
        <%= errorMsg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
<% } %>
   